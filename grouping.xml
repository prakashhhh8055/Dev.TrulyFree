<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Grouping">


  <groups>
	  <run>
		  <!--<include name="Smoke"/>-->
		  <include name="Sanity"/>
		  <!--<include name="Regression"/> -->
		  <!-- <exclude name="" /> -->
	  </run>
  </groups>
  
   <listeners>
